{
	"name": "pupil",
	"displayName": "Pupil",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.103.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [],
		"customEditors": [
			{
				"viewType": "pupil.editor",
				"displayName": "Pupil Editor",
				"selector": [
					{
						"filenamePattern": "*.js"
					},
					{
						"filenamePattern": "*.jsx"
					},
					{
						"filenamePattern": "*.tsx"
					},
					{
						"filenamePattern": "*.ts"
					},
					{
						"filenamePattern": "*.html"
					},
					{
						"filenamePattern": "*.css"
					},
					{
						"filenamePattern": "*.json"
					}
				],
				"priority": "default"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src",
		"test": "vscode-test",
		"dev": "vite dev --config vite.config.ts",
		"build": "vite build --config vite.config.ts"
	},
	"devDependencies": {
		"@types/css-modules": "^1.0.5",
		"@types/mocha": "^10.0.10",
		"@types/node": "22.x",
		"@types/react": "^19.1.11",
		"@types/react-dom": "^19.1.7",
		"@types/vscode": "^1.103.0",
		"@types/webgl2": "^0.0.11",
		"@types/ws": "^8.18.1",
		"@typescript-eslint/eslint-plugin": "^8.39.0",
		"@typescript-eslint/parser": "^8.39.0",
		"@vitejs/plugin-react": "^5.0.0",
		"@vscode/test-cli": "^0.0.11",
		"@vscode/test-electron": "^2.5.2",
		"eslint": "^9.32.0",
		"eslint-plugin-prettier": "^5.5.4",
		"path": "^0.12.7",
		"typescript": "^5.9.2",
		"vite": "^7.1.2"
	},
	"dependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@monaco-editor/react": "^4.7.0",
		"@mui/icons-material": "^7.3.2",
		"@mui/material": "^7.3.2",
		"@tailwindcss/vite": "^4.1.12",
		"express": "^5.1.0",
		"monaco-editor": "^0.52.2",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-simple-keyboard": "^3.8.116",
		"tailwindcss": "^4.1.12",
		"webgazer": "^3.4.0",
		"ws": "^8.18.3"
	},
	"type": "module"
}
